{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "SEOConfig": {
    "SearchURL": "https://www.google.co.uk/search?num=100&q={keyword}",
    "XPathParentList": "//*[@id=\"main\"]",
    "XPathItemNode": "//div"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "bin/debug/net8.0/logs/logs.txt",
          "rollingInterval": "Day"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "SEOWeb"
    }
  },
  "AllowedHosts": "*"
}
